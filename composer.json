{
    "name": "terminal42/contao-facebook-albums",
    "description": "Allow the Contao to display Facebook albums.",
    "keywords": ["contao", "facebook", "album", "gallery", "image"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "http://terminal42.ch"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "contao/core-bundle": "^4.4",
        "facebook/php-sdk-v4": "~5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\FacebookAlbumsBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\FacebookAlbumsBundle\\ContaoManager\\Plugin"
    }
}
